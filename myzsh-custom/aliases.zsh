# Python
alias python=$(brew --prefix)/bin/python3

# Zsh
alias zshconfig="nvim ~/.zshrc"

# Brew
alias brewup="brew update && brew upgrade"
alias brewdoc="brew doctor"
alias brewdump="brew bundle dump"

# Docker
alias dockup="docker compose up"
alias dockdown="docker compose down"

# Vim
alias vim="nvim"
alias vi="nvim"
